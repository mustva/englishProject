@model englishProject.Models.Level
<div class=" alert alert-success alert-update" role="alert" data-bind="visible:end()" style="display: none;">
    <div class="row">
        <div class="col-md-2 text-center">
            <i class="fa fa-thumbs-o-up" style="font-size: 5.7em;"></i>
        </div>
        <div id="warning" class="col-md-7  text-center">
            <h3 style="color: black;margin-top: 0px;"><i class="fa fa-star-o"></i><i class="fa fa-star-o"></i><i class="fa fa-star-o"></i> BRAVO !!!</h3>

            <p>
                @if (Model != null)
                {
                    <strong>Bu seviyenin sonuna kadar geldin. Artık bir sonraki seviyeye geçmeye tam anlamıyla hazırsın.</strong>
                }
                else
                {
                    <strong>Bu kutudaki tüm seviyeleri başarıyla bitirdin.</strong>
                }
            </p>
        </div>
        <div class="col-md-3 text-center">

            @if (Model != null)
            {
                <a href="@Url.Action("LevelExam","User",new{Model.levelId,subLevel=1})" class=" btn btn-update btn-lg"><strong> @Model.levelNumber . seviyeye geç</strong> <i class="fa fa-arrow-right"></i></a>
            }
            else
            {
                <a href="@Url.Action("Index","User")" class=" btn btn-update"><strong>Diğer kutuları gör</strong> <i class="fa fa-arrow-right"></i></a>
            }
        </div>
    </div>
</div>